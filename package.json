{
  "name": "travel-expenses-tracker-backend",
  "version": "1.0.0",
  "description": "A simple http server: hello world",
  "main": "app.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "test": "jest",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/testing": "^8.2.5",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
